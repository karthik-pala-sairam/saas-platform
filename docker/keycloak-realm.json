{
  "realm": "saas-platform",
  "enabled": true,
  "loginTheme": "frontend",
  "roles": {
    "realm": [
      {
        "name": "saas-user"
      }
    ]
  },
  "clients": [
    {
      "clientId": "frontend-angular",
      "protocol": "openid-connect",
      "publicClient": true,
      "redirectUris": [
        "http://localhost:4200",
        "http://localhost:4200/",
        "http://localhost:4200/*",
        "http://127.0.0.1:4200",
        "http://127.0.0.1:4200/",
        "http://127.0.0.1:4200/*"
      ],
      "webOrigins": [
        "http://localhost:4200",
        "http://127.0.0.1:4200"
      ]
    }
  ],
  "users": [
    {
      "username": "testuser",
      "enabled": true,
      "email": "testuser@example.com",
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "realmRoles": [
        "saas-user"
      ]
    }
  ]
}
